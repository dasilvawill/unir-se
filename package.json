{
  "name": "solidarize",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "services:up": "docker compose -f src/app/database/compose.yaml up -d",
    "services:down": "docker compose -f src/app/database/compose.yaml down",
    "services:stop": "docker compose -f src/app/database/compose.yaml stop",
    "dev": "npm run services:up && next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@prisma/client": "^5.13.0",
    "next": "14.2.3",
    "prisma": "^5.13.0",
    "react": "^18",
    "react-dom": "^18",
    "zod": "^3.23.6"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.2.3",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
